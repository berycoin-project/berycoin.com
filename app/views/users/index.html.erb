<div class="container">
  <div class="row">
    <h3><i class="fa fa-users"></i>Users</h3>
    <div class="column">
      <table class="table">
        <tbody>
          <% @users.each do |user| %>
            <tr>
              <%= render user %>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>

<% content_for :title do %>
  <i class='fa fa-users'></i> <%= "Users" %>
<% end %>
